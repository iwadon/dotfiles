[user]
	name = Hiroyuki Iwatsuki
	email = don@na.rim.or.jp
[color]
	ui = true
[alias]
	co = checkout
	ci = commit
[push]
	default = simple
[core]
	excludesfile = ~/.gitignore
	quotepath = false
	pager = lv -c -Ou8
[include]
	path = ~/.gitconfig.github
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
